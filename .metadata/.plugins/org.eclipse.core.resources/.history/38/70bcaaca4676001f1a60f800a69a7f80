<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oracle.samil.salDao">

	<select id="salTotal" resultType="int">
		SELECT Count(*) FROM emp
	</select>
	
		<select id="empTotal" parameterType="Emp" resultType="Emp">
		SELECT
		*
		FROM
		(
		SELECT rownum rn , a.*
		FROM
		(select * from emp order by empno) a
		)
		WHERE rn BETWEEN #{start} and #{end}

	</select>
	
	<select id="pagingSelect" parameterType="paging" resultType="datas">
		
		select * from emp
		
	</select>


</mapper>
        