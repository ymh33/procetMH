<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oracle.samil.salDao">

	<select id="salTotal" resultType="int">
		SELECT Count(*) FROM emp
	</select>
	
		<select id="empTotal" parameterType="Emp" resultType="Emp">
		select e.empno, e.name, e.grade, e.deptno, e.hiredate, d.deptName
		 from emp e , dept d 
		 where e.deptno = d.deptno(+)
		 order by empno

	</select>
	
	<select id ="salDList" parameterType="Sal" resultType="Sal">
	
	select * from sal where #{empno}
	
	</select>



</mapper>
        